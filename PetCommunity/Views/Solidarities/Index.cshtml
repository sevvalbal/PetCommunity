<main>
    <div class="row">
        <div class="col-3"></div>
        <div class="col-6">

            <h1 style="text-align: center;">Yardım Talepleri</h1>
            <p>Yardıma ihtiyacı olan evcil hayvan sahiplerini görüntülemektesiniz. Aşağıda yer alan yardım taleplerine destek olabilecekseniz iletişime geçin.</p>
            <hr>
            <a asp-controller="Solidarities" asp-action="Create">
                <img src="data:image/png;base64," width="100%" alt="Alternate Text" class="aw-zoom" />

            </a>
            <br>
            <div class="row">
                <div class="col-6">
                </div>
                <div class="col-6">
                    <div style="text-align: right;">
                    </div>
                </div>

            </div>



            @foreach (var item in Model)
            {


                <br>
                <div class="shop">
                    <h4 style="color: #1c7c54"> @Html.DisplayFor(modelItem => item.Name) @Html.DisplayFor(modelItem => item.LastName) Yardım Arıyor!</h4>
                    <hr>
                    <div class="row">
                        <div class="col-12">
                            <h5 style="color: #0077b6">Yardım Talebi Detayları</h5>
                            <p><b>Yardım Türü: </b>@Html.DisplayFor(modelItem => item.Help_Type)</p>
                            <p><b>Açıklama: </b>@Html.DisplayFor(modelItem => item.Description)</p>
                            <hr>
                            <h5 style="color: #0077b6">İletişim Bilgileri</h5>
                            <p><b>İsim - Soyisim: </b>@Html.DisplayFor(modelItem => item.Name) @Html.DisplayFor(modelItem => item.LastName)</p>
                            <p><b>Telefon Numarası: </b>@Html.DisplayFor(modelItem => item.PhoneNumber)</p>
                            <p><b>E-Posta Adresi: </b>@Html.DisplayFor(modelItem => item.EMail)</p>
                            <hr>
                            <h5 style="color: #0077b6">Konum Bilgileri</h5>
                            <p><b>Şehir: </b>@Html.DisplayFor(modelItem => item.City)</p>
                            <p><b>Adres: </b>@Html.DisplayFor(modelItem => item.Address)</p>
                            <hr>
                            <p style="color: #1c7c54">Yardım talebine destek sağlayabilecekseniz iletişime geçebilirsiniz.</p>
                        </div>
                    </div>

                </div>



            }

        </div>
    </div>

</main>